<resources>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <color name="colorAccent">#FF4081</color>

    <color name="colorBackgroundNoteBody">#E8EAF6</color>
    <color name="colorBackgroundNoteTitle">#DEE0EA</color>
    <color name="colorNoteItemSeparator">#CCCCCC</color>
    <color name="colorNoteSelectedItem">#C5CAE9</color>

    <!--
    First 2 values represent opacity in hex (base16)
    For 80% opacity: 0.8 * 255 = 204 (CC in hex)
    -->
    <color name="colorFontNormal">#CC000000</color>
    <!-- 0.7 * 255 = 179 (B3) -->
    <color name="colorFontSemibold">#B3000000</color>

    <!-- 66% opacity (170 or AA in hex) -->
    <color name="colorFontSub">#AA000000</color>
</resources>
